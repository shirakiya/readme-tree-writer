{
  "name": "readme-tree",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Action to write the output of \"tree\" command to each README in your project.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.{js,ts}'",
    "format:check": "prettier --check '**/*.{js,ts}'",
    "lint": "eslint src/**/*",
    "package": "ncc build src/main.ts --source-map --license licenses.txt",
    "test": "jest src/search.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shirakiya/readme-tree.git"
  },
  "keywords": [
    "actions",
    "README",
    "tree"
  ],
  "author": "shirakiya <shirakiya.pv@gmail.com> (https://github.com/shirakiya)",
  "license": "MIT",
  "homepage": "https://github.com/shirakiya/readme-tree#readme",
  "bugs": {
    "url": "https://github.com/shirakiya/readme-tree/issues"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.3.0",
    "js-yaml": "^4.1.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.8.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
